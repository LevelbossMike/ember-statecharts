{
  "name": "ember-statecharts",
  "version": "0.11.0",
  "description": "Statecharts for Ember.js applications",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/LevelbossMike/ember-statecharts",
  "license": "MIT",
  "types": "./index.d.ts",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "lerna-changelog",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@glimmer/tracking": "^1.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli-babel": "^7.18.0",
    "ember-cli-htmlbars": "^4.2.3",
    "ember-usable": "https://github.com/pzuraq/ember-usable#0d03a50",
    "xstate": "^4.8.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@ember/render-modifiers": "^1.0.2",
    "@glimmer/component": "^1.0.0",
    "@release-it/conventional-changelog": "^1.1.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-angle-bracket-invocation-polyfill": "^2.0.2",
    "ember-cli": "~3.17.0",
    "ember-cli-addon-docs": "^0.6.16",
    "ember-cli-addon-docs-esdoc": "^0.2.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-concurrency": "^1.1.6",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-qunit-assert-helpers": "^0.2.2",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.20.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.4.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-ember": "^7.10.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "qunit-dom": "^1.1.0",
    "release-it": "^13.6.2"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "changelog": {
    "repo": "levelbossmike/ember-statecharts",
    "labels": {
      ":boom: Breaking": ":boom: Breaking Change",
      "Feature / Enhancement": ":rocket: Feature / Enhancement",
      "Bug": ":bug: Bug Fix",
      "Documentation": ":memo: Documentation",
      "internal": ":house: Internal"
    }
  },
  "homepage": "https://ember-statecharts.com/",
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  }
}
