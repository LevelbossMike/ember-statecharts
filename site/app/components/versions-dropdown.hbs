<div>
  {{#if this.isSuccess}}
    <select
      name='versions'
      id=''
      class='text-white bg-transparent focus:outline-none cursor-pointer ml-2 md:ml-0 text-sm md:text-base'
      {{on 'change' this.redirectTo}}
    >
      {{#each-in this.versions as |key value|}}
        <option value={{value.path}} selected={{is-current-version value.path}}>
          {{value.label}}
        </option>
      {{/each-in}}
    </select>
  {{/if}}
</div>
